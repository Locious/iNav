<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.inav"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/inav_logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.inav.Splash"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.inav.Home"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.inav.TempPage"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.inav.DeadReckon"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.inav.Navigate"
            android:label="@string/title_activity_navigate" >
        </activity>
    </application>

</manifest>
